// e03_invalid_operator

class ErrorTest {
    public static void main (String[] args){
        //int x;
        x = 5 # 3; // Erro: '#' não é um operador válido
        x = 5 @ 3; // Erro: '@' não é um operador válido
        x = 5 & 3; // Erro: '&' não é um operador válido
        x = 5 ? 3; // Erro: '?' não é um operador válido
        x = 5 ¬ 3; // Erro: '¬' não é um operador válido
    }
}