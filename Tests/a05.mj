// 05_statements_control_flow

class ControlFlowTest {
    public static void main (String[] args){
        int x;
        x = 0;
        if (x < 10) {
            System.out.println(x);
        } else {
            System.out.println(10);
        }

        while (x < 5) {
            System.out.println(x);
            x = x + 1;
        }
    }
}